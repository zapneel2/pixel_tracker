const fs = require('fs'); const path = require('path');exports.handler = async (event, context) = const filePath = path.resolve(__dirname, '../static/pixel.png');  const pixel = fs.readFileSync(filePath);  console.log('Pixel requested from:', event.headers['user-agent']);  return {    statusCode: 200,    headers: {      'Content-Type': 'image/png',      'Cache-Control': 'no-store',    },    body: pixel.toString('base64'),    isBase64Encoded: true,  };};
